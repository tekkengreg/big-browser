{
  "name": "@tekkengreg/big-browser",
  "version": "1.0.0",
  "description": "Electron app loading url",
  "main": "src/main.js",
  "scripts": {
    "start": "electron ./src/main.js --no-sandbox",
    "build": "electron-builder",
    "build:flatpak:notion": "flatpak-builder --repo=repo --force-clean build-dir com.tekkengreg.bigbrowser.notion.yml",
    "build:flatpak:google": "flatpak-builder --repo=repo --force-clean build-dir com.tekkengreg.bigbrowser.google.yml",
    "build:flatpak:sketchup": "flatpak-builder --repo=repo --force-clean build-dir com.tekkengreg.bigbrowser.sketchup.yml",
    "build:flatpak:all": "pnpm build:flatpak && pnpm build:flatpak:google && pnpm build:flatpak:sketchup"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "electron": "^35.5.1",
    "electron-builder": "^26.0.12"
  },

  "pnpm": {
    "onlyBuiltDependencies": [
      "electron"
    ]
  },
 
  "packageManager": "pnpm@10.12.1+sha512.f0dda8580f0ee9481c5c79a1d927b9164f2c478e90992ad268bbb2465a736984391d6333d2c327913578b2804af33474ca554ba29c04a8b13060a717675ae3ac"
}
